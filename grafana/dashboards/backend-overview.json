{
  "id": null,
  "uid": "backend-overview",
  "title": "WorkoutApp Backend Overview",
  "tags": ["workoutapp", "backend"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "job",
        "label": "Service",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(up, job)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": {}
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Requests per second by service",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "options": {
        "legend": {"showLegend": true},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=~\"$job\"}[5m])) by (job)",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Error rate (5xx) by service",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      },
      "options": {
        "legend": {"showLegend": true},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status=~\"5..\",job=~\"$job\"}[5m])) by (job) / sum(rate(http_requests_total{job=~\"$job\"}[5m])) by (job)",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Average request duration by service (s)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": {"showLegend": true},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_seconds_sum{job=~\"$job\"}[5m])) by (job) / sum(rate(http_request_duration_seconds_count{job=~\"$job\"}[5m])) by (job)",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Workouts created per minute (all sources)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {"h": 7, "w": 12, "x": 0, "y": 24},
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "options": {
        "legend": {"showLegend": false},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "sum(rate(workouts_created_total[5m]))",
          "legendFormat": "workouts_created",
          "refId": "A"
        }
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Workout sessions finished per minute",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {"h": 7, "w": 12, "x": 12, "y": 24},
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "options": {
        "legend": {"showLegend": false},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "sum(rate(workout_sessions_finished_total[5m]))",
          "legendFormat": "sessions_finished",
          "refId": "A"
        }
      ]
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Training plans generated per minute",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {"h": 7, "w": 12, "x": 0, "y": 31},
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "options": {
        "legend": {"showLegend": true},
        "tooltip": {"mode": "multi"}
      },
      "targets": [
        {
          "expr": "sum(rate(training_plans_generated_total[5m])) by (variant)",
          "legendFormat": "{{variant}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Calendar plans created per minute",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {"h": 7, "w": 12, "x": 12, "y": 31},
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "options": {
        "legend": {"showLegend": false},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "sum(rate(calendar_plans_created_total[5m]))",
          "legendFormat": "calendar_plans",
          "refId": "A"
        }
      ]
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "CRM links created per minute",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": {"h": 7, "w": 12, "x": 0, "y": 38},
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "options": {
        "legend": {"showLegend": false},
        "tooltip": {"mode": "single"}
      },
      "targets": [
        {
          "expr": "sum(rate(crm_links_created_total[5m]))",
          "legendFormat": "crm_links",
          "refId": "A"
        }
      ]
    }
  ]
}
